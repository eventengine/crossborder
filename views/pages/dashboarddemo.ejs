<!DOCTYPE html>
<html>

<% include ../partials/stylesheetLinks %>

<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <% include ../partials/header %>

  <% include ../partials/nav %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Dashboard
        <small>Track your inventory.</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="box">
            <div class="box-header with-border">
              <h3 class="box-title">Inventory</h3>
            </div>
        <div class="box-body">
              <table class="table table-bordered">
                <tr>
                  <th>Item</th>
                  <th>Quantity</th>
                  <th>Units</th>
                  <th>Progress</th>
                  <th style="width: 40px">Reorder</th>
                </tr>

                <tr>
                  <td>Quality Cotton</td>
                  <td>60</td>
                  <td>meters</td>
                  <td>
                    <div class="progress progress-xs">
                      <div class="progress-bar progress-bar-success" style="width: 85%"></div>
                    </div>
                  </td>
                  <td></td>
                </tr>

                <tr>
                  <td>Cotton Flannel</td>
                  <td>45</td>
                  <td>meters</td>
                  <td>
                    <div class="progress progress-xs">
                      <div class="progress-bar progress-bar-warning" style="width: 58%"></div>
                    </div>
                  </td>
                  <td></td>
                </tr>

                <tr>
                  <td>One-gallon Ziploc Bag</td>
                  <td>30</td>
                  <td>unit</td>
                  <td>
                    <div class="progress progress-xs">
                      <div class="progress-bar progress-bar-danger" style="width: 11%"></div>
                    </div>
                  </td>
                  <td><div style="padding-left: 30%"><span class="badge bg-red">!</span></div></td>
                </tr>

                <tr>
                  <td>5/8" Blue Ribbon</td>
                  <td>56</td>
                  <td>meters</td>
                  <td>
                    <div class="progress progress-xs">
                      <div class="progress-bar progress-bar-success" style="width: 71%"></div>
                    </div>
                  </td>
                  <td></td>
                </tr>

                <tr>
                  <td>Yellow Flannel</td>
                  <td>38</td>
                  <td>meters</td>
                  <td>
                    <div class="progress progress-xs">
                      <div class="progress-bar progress-bar-warning" style="width: 45%"></div>
                    </div>
                  </td>
                  <td></td>
                </tr>

                <tr>
                  <td>Green Snaps</td>
                  <td>127</td>
                  <td>unit</td>
                  <td>
                    <div class="progress progress-xs">
                      <div class="progress-bar progress-bar-success" style="width: 96%"></div>
                    </div>
                  </td>
                  <td></td>
                </tr>

                

              </table>
            </div>
          </div>
          <!-- /.box -->

    </section>
    <!-- /.content -->
  </div>

</div>
<!-- ./wrapper -->

<% include ../partials/scripts %>

</body>
</html>
